{
  "name": "js-vue-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "depcheck": "depcheck ",
    "dev": "vite",
    "format": "pnpm run format/js && pnpm run format/properties && pnpm run format/yaml",
    "format/js": "prettier --write \"./**/*.{js,ts,css,json}\" --config ./.prettierrc",
    "format/properties": "find . -name '*.properties' -print -exec sort -o {} {} \\;",
    "format/yaml": "git ls-files '*.yml' '*.yaml' | xargs -t -I {} yq -i -S -Y . {}",
    "lint": "eslint src/*.js",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "maplibre-gl": "^3.6.2",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.19.2",
    "naive-ui": "^2.36.0",
    "prettier": "^3.1.0",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-sort-json": "^3.1.0",
    "vite": "^5.0.8"
  }
}
